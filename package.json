{
  "name": "react-assignment",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-scripts": "1.0.14",
    "rebass": "^1.0.4",
    "redux": "^3.7.2",
    "styled-components": "^2.2.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
